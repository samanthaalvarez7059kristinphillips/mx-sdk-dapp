{
  "name": "@elrondnetwork/dapp-core",
  "version": "2.0.0-alpha.3",
  "description": "A library to hold the main logic for a dapp on the Elrond Network",
  "author": "ElrondNetwork",
  "license": "GPL-3.0-or-later",
  "repository": "ElrondNetwork/dapp-core",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "npm run watch",
    "build:types": "tsc --project tsconfig.json",
    "build": "webpack",
    "test": "dts test --passWithNoTests",
    "lint": "dts lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": [
    "dist/*.js",
    "dist/**/*.js",
    "dist/*.d.ts",
    "dist/**/*.d.ts"
  ],
  "keywords": [
    "elrond",
    "blockchain",
    "dapp",
    "smart-contract",
    "transaction"
  ],
  "peerDependencies": {
    "react": ">=18"
  },
  "resolutions": {
    "@types/react": "18.0.10",
    "@types/react-dom": "18.0.5"
  },
  "optionalDependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.0",
    "@fortawesome/free-solid-svg-icons": "6.1.0",
    "@fortawesome/react-fontawesome": "0.1.18",
    "bootstrap": "4.6.0",
    "classnames": "2.3.1",
    "moment": "2.29.1",
    "platform": "1.3.6",
    "qrcode": "1.5.0",
    "react-bootstrap": "2.0.3",
    "swr": "1.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/dapp-core.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/dapp-core.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@elrondnetwork/erdjs": "10.2.5",
    "@elrondnetwork/erdjs-extension-provider": "2.0.1",
    "@elrondnetwork/erdjs-hw-provider": "2.0.1",
    "@elrondnetwork/erdjs-network-providers": "0.1.5",
    "@elrondnetwork/erdjs-wallet-connect-provider": "2.0.2",
    "@elrondnetwork/erdjs-walletcore": "1.0.0",
    "@elrondnetwork/erdjs-web-wallet-provider": "2.1.0",
    "@reduxjs/toolkit": "1.8.2",
    "autoprefixer": "9.8.8",
    "axios": "0.24.0",
    "bignumber.js": "9.x",
    "lodash.debounce": "4.0.8",
    "lodash.isequal": "4.5.0",
    "lodash.omit": "4.5.0",
    "lodash.throttle": "4.1.1",
    "qs": "6.10.3",
    "react-error-overlay": "6.0.9",
    "react-idle-timer": "5.0.0",
    "react-redux": "8.0.2",
    "redux-persist": "6.0.0",
    "reselect": "4.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss-modules": "2.0.2",
    "shx": "0.3.4"
  },
  "yalcSig": "05f619ffa380a82f257d1e8c37e2e1eb"
}
