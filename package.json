{
  "name": "@elrondnetwork/dapp-core",
  "version": "1.1.4",
  "description": "A library to hold the main logic for a dapp on the Elrond Network",
  "author": "ElrondNetwork",
  "license": "GPL-3.0-or-later",
  "repository": "ElrondNetwork/dapp-core",
  "main": "dist/index.js",
  "module": "dist/dapp-core.esm.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "npm run copy-dev & tsdx watch",
    "build": "npm run copy-prod & tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "copy-dev": "cp src/optionalPackages/dev/* src/optionalPackages",
    "copy-prod": "cp src/optionalPackages/prod/* src/optionalPackages"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@size-limit/preset-small-lib": "7.0.8",
    "@svgr/rollup": "6.2.1",
    "@types/lodash": "4.14.181",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.43",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.isequal": "4.5.5",
    "@types/lodash.omit": "4.5.6",
    "@types/lodash.throttle": "4.1.6",
    "@types/react-dom": "17.0.0",
    "babel-loader": "8.2.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react": "1.1.7",
    "eslint-config-react-app": "6.0.0",
    "eslint-config-standard": "16.0.2",
    "eslint-config-standard-react": "9.2.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "18.0.0",
    "rollup-plugin-scss": "3.0.0",
    "rollup-plugin-svg-import": "1.6.0",
    "sass": "1.32",
    "size-limit": "7.0.8",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.6.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "elrond",
    "blockchain",
    "dapp",
    "smart-contract",
    "transaction"
  ],
  "peerDependencies": {
    "react": "~17.0"
  },
  "optionalDependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.0",
    "@fortawesome/free-solid-svg-icons": "6.1.0",
    "@fortawesome/react-fontawesome": "0.1.18",
    "bootstrap": "4.6.0",
    "classnames": "2.3.1",
    "moment": "2.29.1",
    "platform": "1.3.6",
    "qrcode": "1.5.0",
    "react-bootstrap": "2.0.3",
    "swr": "1.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/dapp-core.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/dapp-core.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@elrondnetwork/erdjs": "9.1.0",
    "@reduxjs/toolkit": "1.6.2",
    "@testing-library/jest-dom": "5.16.2",
    "axios": "0.24.0",
    "bignumber.js": "9.x",
    "lodash.debounce": "4.0.8",
    "lodash.isequal": "4.5.0",
    "lodash.omit": "4.5.0",
    "lodash.throttle": "4.1.1",
    "qs": "6.10.3",
    "react-error-overlay": "6.0.9",
    "react-redux": "7.2.6",
    "react-scripts": "4.0.3",
    "redux-persist": "6.0.0",
    "reselect": "4.0.0"
  }
}
